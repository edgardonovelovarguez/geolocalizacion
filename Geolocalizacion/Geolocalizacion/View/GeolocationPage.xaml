<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Geolocalizacion.ViewModel"
             x:Class="Geolocalizacion.View.GeolocationPage">
    <ContentPage.BindingContext>
        <local:GeolocationViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout Margin="20">
            <!--Entry Text="{Binding Latitud}"/>
            <Entry Text="{Binding Longitud}"/-->
            <Label Text="{Binding DisplayCor}"
                   VerticalOptions="Center"
                   HorizontalOptions="Center" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>